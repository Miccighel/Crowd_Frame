{
    "/brave": {
        "target": "https://api.search.brave.com",
        "secure": true,
        "changeOrigin": true,
        "logLevel": "debug",
        "pathRewrite": {
            "^/brave": "/res/v1/web/search"
        }
    },
    "/google": {
        "target": "https://www.googleapis.com",
        "secure": true,
        "changeOrigin": true,
        "pathRewrite": {
            "^/google": "/customsearch/v1"
        }
    }
}
